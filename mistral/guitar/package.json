{
  "name": "guitar-teacher-agent",
  "version": "1.0.0",
  "description": "A Mistral AI guitar teacher agent with web interface",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "node index.js",
    "api:dev": "cd api && npm run dev",
    "api:install": "cd api && npm install",
    "web:serve": "cd public && npx http-server -p 3001",
    "deploy:vercel": "vercel deploy --prod",
    "deploy:api": "cd api && vercel deploy --prod"
  },
  "keywords": [
    "mistral",
    "ai",
    "guitar",
    "teacher",
    "blues",
    "rock",
    "web-interface"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.20.0",
  "dependencies": {
    "@mistralai/mistralai": "^1.14.0",
    "dotenv": "^17.3.1"
  }
}
